{
  "name": "remark-generic-extensions",
  "description": "remark-react example with react-toolbox TooltipIcon",
  "version": "1.1.0",
  "dependencies": {
    "remark-react": "4.0.0",
    "remark-generic-extensions": "1.1.0",
    "remark": "7.0.1",
    "react-toolbox": "2.0.0-beta.12",
    "react-dom": "15.5.3",
    "react": "15.5.3",
    "hast-util-sanitize": "1.1.1",
    "deepmerge": "1.5.0"
  },
  "devDependencies": {
    "react-toolbox-themr": "^1.0.2"
  },
  "reactToolbox": {
    "include": [
      "BUTTON",
      "TOOLTIP"
    ],
    "customProperties": {
      "tooltip-color": "#000"
    },
    "javascript": "src/assets/react-toolbox/theme.js",
    "styles": "src/assets/react-toolbox/theme.css"
  },
  "scripts": {
    "toolbox": "react-toolbox-themr"
  }
}
